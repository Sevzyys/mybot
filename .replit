run = "npm start"
modules = ["nodejs-20"]

[[ports]]
localPort = 3000
externalPort = 80

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Run Discord Bot"

[[workflows.workflow]]
name = "Run Discord Bot"
author = 44354705
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node index.js"

[deployment]
run = ["sh", "-c", "node index.js"]
deploymentTarget = "gce"
ignorePorts = true
